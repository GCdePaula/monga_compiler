

main () : int {
  i : int;
  j : int;

  f : float;
  ff : float;

  i = 0 + 0; @i;
  i = 1 + 2; @i;
  i = i + 1; @i;
  i = i + i; @i;

  j = i - i; @j;
  j = 0 * 0; @j;
  j = 3 * 3; @j;
  j = 3 / 3; @j;


  f = .0 + 0; @f;
  f = 1. + 2; @f;
  f =  f + 1; @f;
  f =  f + f; @f;

  ff = f - f; @ff;
  ff = .0 * 0; @ff;
  ff = 3. * 3; @ff;
  ff = 3. / 3; @ff;

  return 0;
}
