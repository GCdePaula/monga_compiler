
main () : int {

  @ "Gt e Ge";
  @ 10 > 9;
  @ 9 > 10;
  @ 10 >= 10;
  @ 10 > 10;

  @ 10. > 9;
  @ 9. > 10;
  @ 10. >= 10;
  @ 10. > 10;

  @ 10. > 9.;
  @ 9. > 10.;
  @ 10. >= 10.;
  @ 10. > 10.;

  @ "Lt e Le";
  @ 9 < 10;
  @ 10 < 9;
  @ 10 <= 10;
  @ 10 < 10;

  @ 9. < 10;
  @ 10. < 9;
  @ 10. <= 10;
  @ 10. < 10;

  @ 9. < 10.;
  @ 10. < 9.;
  @ 10. <= 10.;
  @ 10. < 10.;

  @ "Eq";
  @ 10 == 10;
  @ 10 == 11;
  @ 10. == 10;
  @ 10. == 11;
  @ 10. == 10.;
  @ 10. == 11.;
  @ true == true;
  @ true == false;

  @ "Ne";
  @ 10 ~= 11;
  @ 10 ~= 10;
  @ 10. ~= 11;
  @ 10. ~= 10;
  @ 10. ~= 11.;
  @ 10. ~= 10.;
  @ true ~= false;
  @ true ~= true;

  @ "And";
  @ true && true;
  @ true && false;
  @ false && false;
  @ false && true;

  @ true && true && false;
  @ true && false && true;
  @ false && true && true;
  @ true && true && true;

  @ "Or";
  @ true || true;
  @ true || false;
  @ false || false;
  @ false || true;

  @ false || false || true;
  @ false || true || false;
  @ true || false || false;
  @ false || false || false;

  @ "Not";
  @ !true;
  @ !false;

  return 0;
}
